# React Dashboard Environment Configuration
# Copy this file to .env.local and customize as needed

# MCP Server Connection
REACT_APP_MCP_SERVER_URL=ws://localhost:8765
REACT_APP_MCP_SERVER_HOST=localhost
REACT_APP_MCP_SERVER_PORT=8765

# Dashboard Configuration
REACT_APP_UPDATE_INTERVAL=2000
REACT_APP_DEFAULT_THEME=light
REACT_APP_MAX_LOG_ENTRIES=500
REACT_APP_MAX_PROCESS_ENTRIES=20

# Chart Configuration
REACT_APP_CHART_UPDATE_INTERVAL=1000
REACT_APP_CHART_DATA_POINTS=50

# Alert Configuration
REACT_APP_ENABLE_SOUND_ALERTS=false
REACT_APP_ALERT_TOAST_DURATION=5000

# Development
REACT_APP_DEBUG=false
GENERATE_SOURCEMAP=false